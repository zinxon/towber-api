{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "towber-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": ["nodejs_compat_v2"],
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  "vars": {
  },
  
    "r2_buckets": [
      {
        "binding": "BUCKET",
        "bucket_name": "towber",
        "preview_bucket_name": "towber-dev"
      }
    ]
}